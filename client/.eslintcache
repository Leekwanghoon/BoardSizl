nt: center;\r\n    align-items: center;\r\n`;\r\n\r\n\r\nconst LandingPage = (props) => {\r\n    \r\n    const welcomeHelmet = \"환영합니다\";\r\n\r\n    const dispatch = useDispatch();\r\n    const [Loading, setLoading] = useState(true);\r\n    const [BoardInfo, setBoardInfo] = useState([]); //토탈정보\r\n    const [Skip, setSkip] = useState(0);\r\n    const [Limit, setLimit] = useState(4);\r\n\r\n    useEffect(() => {\r\n        let body = {\r\n            skip:Skip,\r\n            limit:Limit\r\n        };\r\n        getBoards(body);\r\n    },[Skip,Limit])\r\n\r\n    const getBoards = (body) => {\r\n        axios.post('/api/getBoards', body)\r\n            .then(response => {\r\n                if(response.data.success) {\r\n                    setBoardInfo(response.data.boardInfo);\r\n                    setLoading(false);\r\n                } else {\r\n                    alert(\"게시글 가져오기 실패!\");\r\n                }\r\n            })\r\n    }\r\n\r\n    const onLogoutButton = () => {\r\n        dispatch(logoutUser())\r\n            .then(response => {\r\n                if(response.payload.success) {\r\n                    props.history.push(\"/\");\r\n                } else {\r\n                    alert(\"Failed to logout\");\r\n                }\r\n            })\r\n    }\r\n    const onGoLoginPage = () => {\r\n        props.history.push(\"/login\");\r\n    }\r\n\r\n    const onClickPage = useCallback((pageNum) => {\r\n        console.log(pageNum,\"checking\")\r\n        const pageNum1 = parseInt(pageNum) + 1;\r\n        const skip = 4*(pageNum1-2); \r\n        let body = {\r\n            skip,\r\n            limit: Limit\r\n        }\r\n        getBoards(body);\r\n        setSkip(skip);\r\n        \r\n    },[Limit])\r\n\r\n    return( \r\n        <>\r\n        {!Loading ? \r\n            <Wrapper>\r\n                <Helmet>\r\n                    <title>\r\n                        Board(SIZL) | {props?.user?.userData?.name ? props.user.userData.name : welcomeHelmet}\r\n                    </title>\r\n                </Helmet>\r\n                    <ButtonControl>\r\n                        {props?.user?.userData?.isAuth ? \r\n                            <InterativeButton onClick={onLogoutButton} text=\"로그아웃\" /> : \r\n                            <InterativeButton onClick={onGoLoginPage} text=\"로그인으로 가기\" />\r\n                        }\r\n                    </ButtonControl>\r\n                    <Container>\r\n                        <Board BoardInfo={BoardInfo} onClickPage={onClickPage} />\r\n                    </Container>\r\n            </Wrapper> \r\n            :\r\n            <Wrapper>\r\n                <Helmet>\r\n                   <title>SIZL | Loading...</title>\r\n                </Helmet>\r\n                <LoadingPage />\r\n            </Wrapper>\r\n        }\r\n        </>\r\n    );\r\n}\r\n\r\nexport default React.memo(withRouter(LandingPage));","C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\utils\\Icons.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\components\\views\\Board\\BoardDetailPage.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\utils\\LoadingPage.js",[],["73","74"],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\Styles\\GlobalStyles.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\components\\views\\utilsComponents\\FileUpload.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\components\\views\\UploadPage\\UploadPage.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\components\\views\\Board\\BoardUpdate.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\App.js",["75","76"],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\components\\views\\Board\\BoardEdit.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\utils\\FatText.js",["77"],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\client\\src\\utils\\InterativeButton.js",[],"C:\\Users\\vkdnj\\Desktop\\login구현\\login\\clie